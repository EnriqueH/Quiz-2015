<!-- Formulario para buscar preguntas con el texto introducido en la busqueda -->
<form method="get" action="/quizes">
  <input type="text" name="search" value="Pregunta a buscar" />
  <input type="submit" value="Buscar" />
</form>

<!-- Tabla generada por el bucle for que muestra las preguntas -->
<table>
  <% var i; for (i=0; i < quizes.length; i++) { %>
    <tr><td><a href="quizes/<%= quizes[i].id %>"><%=quizes[i].pregunta %></a></td></tr>
  <% } %>
</table>
